# Instance-specific EntityDB configuration
# This overrides the defaults in share/config/entitydb_server.env

# Enable SSL for this instance
ENTITYDB_USE_SSL=true

# Use default port for HTTP (redirected to HTTPS)
ENTITYDB_PORT=8085

# Use standard HTTPS port for SSL
ENTITYDB_SSL_PORT=8085

# Path to SSL certificate
ENTITYDB_SSL_CERT=/etc/ssl/certs/server.pem

# Path to SSL private key
ENTITYDB_SSL_KEY=/etc/ssl/private/server.key

# Set info logging
ENTITYDB_LOG_LEVEL=info
